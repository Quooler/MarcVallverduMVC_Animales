@model List<Animal>

<div>
    <h1 class="text-center">@ViewBag.Mensaje</h1><br />

    <ul>
        @foreach (Animal animal in Model)
        {
            <li>Id: @animal.IdAnimal Nombre: @animal.Nombre</li>   
        } 
    </ul><br />

    <h1 class="text-center">@ViewBag.Ejemplo</h1>
</div>